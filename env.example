# Database connection string for Prisma ORM
# Format: "postgresql://user:password@host:port/database?schema=public"
# For SQLite (development): "file:./dev.db"
DATABASE_URL=

# Secret key for NextAuth.js session encryption
# Generate a strong random string for production
NEXTAUTH_SECRET=

# Base URL for NextAuth.js callbacks
# Set to your domain in production (e.g., "https://yourdomain.com")
# For local development: "http://localhost:3000"
NEXTAUTH_URL=
